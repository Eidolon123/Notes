{
  "command_timeout": 60,
  "templates_folder": "digital_garden/templates",
  "templates_pairs": [
    [
      "timeNow",
      "date \"+%H:%M\""
    ],
    [
      "timeAfterWork",
      "date -d '25 minutes' \"+%H:%M\""
    ],
    [
      "timeAfterBreak",
      "date -d '32 minutes' \"+%H:%M\""
    ]
  ],
  "trigger_on_file_creation": true,
  "enable_system_commands": true,
  "shell_path": "/bin/zsh",
  "user_scripts_folder": "",
  "enable_folder_templates": true,
  "folder_templates": [
    {
      "folder": "",
      "template": ""
    },
    {
      "folder": "",
      "template": ""
    }
  ],
  "syntax_highlighting": true,
  "enabled_templates_hotkeys": [
    "digital_garden/templates/pomodoro.md"
  ],
  "startup_templates": [
    ""
  ]
}